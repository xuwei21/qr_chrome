{
  "manifest_version": 3,
  "name": "二维码生成器",
  "version": "1.0",
  "description": "快速生成和管理二维码",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "二维码生成器"
  },
  "permissions": [
    "storage"
  ]
}